{
  "name": "Instant Calendar",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Brings the user to calendar.google.com in one click",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}